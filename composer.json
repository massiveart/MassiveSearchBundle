{
    "name": "massive/search-bundle",
    "description": "Massive Search Bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel.leech@massiveart.com"
        }
    ],
    "require": {
        "jms/metadata": "^2.0",
        "symfony/config": "^4.3 || ^5.0",
        "symfony/console": "^4.3 || ^5.0",
        "symfony/event-dispatcher": "^4.3 || ^5.0",
        "symfony/expression-language": "^4.3 || ^5.0",
        "symfony/dependency-injection": "^4.3 || ^5.0",
        "symfony/property-access": "^4.3 || ^5.0",
        "php": ">=7.2.0"
    },
    "require-dev": {
        "handcraftedinthealps/zendsearch": "^2.0",
        "elasticsearch/elasticsearch": "^2.1 || ^5.0",
        "symfony-cmf/testing": "^3.0",
        "symfony/filesystem": "^4.3.2 || ^5.0",
        "symfony/finder": "^4.3 || ^5.0",
        "symfony/http-kernel": "^4.3 || ^5.0",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "symfony/form": "^4.3 || ^5.0",
        "symfony/monolog-bridge": "^4.3 || ^5.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/security-bundle": "^4.3 || ^5.0",
        "symfony/twig-bundle": "^4.3 || ^5.0",
        "symfony/validator": "^4.3 || ^5.0",
        "symfony/phpunit-bridge": "^5.0.4",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0 || ^2.3 || ^3.0 || ^4.0",
        "doctrine/doctrine-cache-bundle": ">= 1.3.1",
        "doctrine/doctrine-bundle": "^1.10 || ^2.0",
        "doctrine/orm": "^2.5",

        "behat/behat": "^3.4.2",

        "guzzlehttp/ringphp": ">= 1.0.7",
        "guzzlehttp/streams": "^3.0.0",
        "webmozart/assert": "^1.7"
    },
    "suggest": {
        "sensio/distribution-bundle": "Required if the SearchScriptHandler is used",
        "handcraftedinthealps/zendsearch": "To use the PHP based Zend Search library (based on Lucene)",
        "elasticsearch/elasticsearch": "To use Elasticsearch"
    },
    "autoload": {
        "psr-4": {
            "Massive\\Bundle\\SearchBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Massive\\Bundle\\SearchBundle\\Tests\\": "Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-release/1.x": "1.0-dev",
            "dev-release/2.x": "2.1-dev",
            "dev-master": "2.2-dev"
        }
    }
}
